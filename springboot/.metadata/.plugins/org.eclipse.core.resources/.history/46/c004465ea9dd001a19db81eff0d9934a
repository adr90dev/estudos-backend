/*
 * Faça um programa em C que calcula e escreve a seguinte soma: soma = 1/1 + 3/2 + 5/3 + 7/4 + ... + 99/50
 */
package revisao;

public class exercicio4 {

	public static void main (String[] args) {
		
		int x=1, y=1;
		float soma = 0;
		
		
		
		for(x=1;x<=99;x++) {
			for(y=1;y<=50;y++) {
				soma = soma + (x/y);
			}
			System.out.println(soma);
		}
	}
}
